<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.amazon.aws.moa.jmsobjects.provisioning.v1_0">
<class name="SamlProvider" table="SAML_PROVIDER">
<id name="AccountId" column="ACCOUNT_ID">
<generator class="assigned"/>
</id>
<property name="Name" column="NAME"/>
<property name="SamlMetadataDocument" type="text" column="SAML_METADATA_DOCUMENT"/>
</class>
</hibernate-mapping>
