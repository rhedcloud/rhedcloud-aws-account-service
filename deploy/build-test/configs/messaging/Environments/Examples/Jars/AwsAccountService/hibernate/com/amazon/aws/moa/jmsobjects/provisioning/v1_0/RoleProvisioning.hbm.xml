<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.amazon.aws.moa.jmsobjects.provisioning.v1_0">
    <class name="RoleProvisioning" table="ROLE_PROVISIONING">
        <id name="RoleProvisioningId" column="ROLE_PROVISIONING_ID">
            <generator class="assigned"/>
        </id>
        <property name="AccountId" column="ACCOUNT_ID" insert="false" update="false"/>
        <component name="RoleProvisioningRequisition" class="com.amazon.aws.moa.objects.resources.v1_0.RoleProvisioningRequisition">
            <property name="AccountId" column="ACCOUNT_ID"/>
            <property name="RoleName" column="ROLE_NAME"/>
            <property name="RoleAssigneeUserId" column="ROLE_ASSIGNEE_USER_ID"/>
        </component>
        <property name="Status" column="STATUS"/>
        <property name="ProvisioningResult" column="PROVISIONING_RESULT"/>
        <property name="ActualTime" column="ACTUAL_TIME"/>
        <property name="AnticipatedTime" column="ANTICIPATED_TIME"/>

        <bag name="RoleProvisioningStep" cascade="all">
            <key column="ROLE_PROVISIONING_ID"/>
            <one-to-many class="com.amazon.aws.moa.objects.resources.v1_0.RoleProvisioningStep"/>
        </bag>

        <property name="CreateUser" column="CREATE_USER"/>
        <property name="CreateDatetime" type="OpenEaiDatetime" column="CREATE_DATETIME"/>
        <property name="LastUpdateUser" column="LAST_UPDATE_USER"/>
        <property name="LastUpdateDatetime" type="OpenEaiDatetime" column="LAST_UPDATE_DATETIME"/>
    </class>
</hibernate-mapping>
