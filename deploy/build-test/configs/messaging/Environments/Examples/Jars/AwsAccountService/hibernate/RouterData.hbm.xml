<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openeai.moa.jmsobjects.coremessaging.v1_0">
<class name="RouterData" table="T_ROUTER_DATA">
<composite-id >
<key-property name="SenderAppId" column="SENDER_APP_ID"/>
<key-property name="MsgId" column="MESSAGE_ID"/>
<key-property name="TargetMessageId" column="TARGET_MESSAGE_ID"/>
</composite-id>
<property name="TargetAppId" column="TARGET_APP_ID"/>
<property name="MsgCategory" column="MESSAGE_CATEGORY"/>
<property name="MsgObject" column="MESSAGE_OBJECT"/>
<property name="MsgAction" column="MESSAGE_ACTION"/>
<property name="MsgType" column="MESSAGE_TYPE"/>
<property name="MsgRelease" column="MESSAGE_RELEASE"/>
<property name="CreateDate" column="CREATE_DATE" type="OpenEaiDate"/>
<property name="CreateUser" column="CREATE_USER"/>
</class>
</hibernate-mapping>
