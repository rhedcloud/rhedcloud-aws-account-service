<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.amazon.aws.moa.objects.resources.v1_0">
<class name="ServiceTestRequirement" table="SERVICE_TEST_REQUIREMENT">

<id name="ServiceTestRequirementId" column="SERVICE_TEST_REQUIREMENT_ID" type="OpenEaiLong">
<generator class="OpenEaiSequence">
<param name="sequence">H_SERVICE_TEST_REQUIREMENT_SEQ</param>
</generator>
</id>
<property name="SequenceNumber" column="SEQUENCE_NUMBER"/>
<property name="Description" column="DESCRIPTION" length="2500"/>
<bag name="ServiceTest" cascade="all">
<key column="SERVICE_TEST_REQUIREMENT_ID"/>
<one-to-many class="com.amazon.aws.moa.objects.resources.v1_0.ServiceTest"/>
</bag>
</class>
</hibernate-mapping>
