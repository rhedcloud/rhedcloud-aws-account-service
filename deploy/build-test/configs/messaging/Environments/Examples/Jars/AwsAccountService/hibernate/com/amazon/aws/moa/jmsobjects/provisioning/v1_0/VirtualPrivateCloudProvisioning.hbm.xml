<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.amazon.aws.moa.jmsobjects.provisioning.v1_0">
<class name="VirtualPrivateCloudProvisioning" table="VRTL_PRVT_CLD_PRVSNNG">
<id name="ProvisioningId" column="PROVISIONING_ID">
<generator class="assigned"/>
</id>

<component name="VirtualPrivateCloudRequisition" class="com.amazon.aws.moa.objects.resources.v1_0.VirtualPrivateCloudRequisition">
<property name="Region" column="REGION"/>
<property name="AccountId" column="ACCOUNT_ID"/>
<property name="RequestId" column="REQUEST_ID"/>
<property name="AuthenticatedRequestorUserId" column="AUTHN_REQUESTOR_USER_ID"/>
<property name="AccountOwnerUserId" column="ACCOUNT_OWNER_USER_ID"/>
<property name="FinancialAccountNumber" column="FINANCIAL_ACCOUNT_NUMBER"/>
<!--TODO: automation-->
<bag name="CustomerAdminUserId" table="VRTL_PRVT_CLD_PRVSNNG_CSTDMND_" >
<key column="PROVISIONING_ID" unique="false"/>
<element type="string" column="CUSTOMER_ADMIN_USER_ID" />
</bag>

<property name="Type" column="TYPE_"/>
<property name="ComplianceClass" column="COMPLIANCE_CLASS"/>

<bag name="SensitiveDataType" table="VRTL_PRVT_CLD_PRVSNNG_SNSTDT_" >
<key column="PROVISIONING_ID" unique="false"/>
<element type="string" column="SENSITIVE_DATA_TYPE" />
</bag>
<property name="Purpose" column="PURPOSE"/>
<property name="NotifyAdmins" column="NOTIFY_ADMINS"/>
</component>
<property name="Status" column="STATUS"/>
<property name="ProvisioningResult" column="PROVISIONING_RESULT"/>
<property name="ActualTime" column="ACTUAL_TIME"/>
<property name="AnticipatedTime" column="ANTICIPATED_TIME"/>

<bag name="ProvisioningStep" cascade="all">
<key column="PROVISIONING_ID"/>
<one-to-many class="com.amazon.aws.moa.objects.resources.v1_0.ProvisioningStep"/>
</bag>

<property name="CreateUser" column="CREATE_USER"/>
<property name="CreateDatetime" type="OpenEaiDatetime" column="CREATE_DATETIME"/>
<property name="LastUpdateUser" column="LAST_UPDATE_USER"/>
<property name="LastUpdateDatetime" type="OpenEaiDatetime" column="LAST_UPDATE_DATETIME"/>
</class>
</hibernate-mapping>
