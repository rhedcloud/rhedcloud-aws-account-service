<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.amazon.aws.moa.objects.resources.v1_0">
<class name="ServiceTestStep" table="SERVICE_TEST_STEP">

<id name="ServiceTestStepId" column="SERVICE_TEST_STEP_ID" type="OpenEaiLong">
<generator class="OpenEaiSequence">
<param name="sequence">H_SERVICE_TEST_STEP_SEQ</param>
</generator>
</id>
<property name="ServiceTestId" column="SERVICE_TEST_ID"/>
<property name="SequenceNumber" column="SEQUENCE_NUMBER"/>
<property name="Description" column="DESCRIPTION" length="40000"/>
</class>
</hibernate-mapping>
