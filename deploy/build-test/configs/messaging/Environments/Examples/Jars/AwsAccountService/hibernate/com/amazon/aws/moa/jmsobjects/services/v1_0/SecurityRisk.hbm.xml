<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.amazon.aws.moa.objects.resources.v1_0">
<class name="SecurityRisk" table="SECURITY_RISK">

<id name="SecurityRiskId" column="SECURITY_RISK_ID" type="OpenEaiLong">
<generator class="OpenEaiSequence">
<param name="sequence">H_SECURITY_RISK_SEQ</param>
</generator>
</id>
<property name="ServiceId" column="SERVICE_ID"/>
<property name="SequenceNumber" column="SEQUENCE_NUMBER"/>
<property name="ServiceRiskName" column="SERVICE_RISK_NAME"/>
<property name="RiskLevel" column="RISK_LEVEL"/>
<property name="Description" column="DESCRIPTION" length="2500"/>
<property name="AssessorId" column="ASSESSOR_ID"/>
<property name="AssessmentDatetime" type="OpenEaiDatetime" column="ASSESSMENT_DATETIME"/>

<bag name="Countermeasure" table="SECURITY_RISK_COUNTER_MEASURE">
<key column="SECURITY_RISK_ID"/>
<composite-element class="com.amazon.aws.moa.objects.resources.v1_0.Countermeasure" >
<property name="Status" column="STATUS"/>
<property name="Description" column="DESCRIPTION" length="2500"/>
<property name="Verifier" column="VERIFIER"/>
<property name="VerificationDatetime" type="OpenEaiDatetime" column="VERIFICATION_DATETIME"/>
</composite-element>
</bag>
</class>
</hibernate-mapping>
